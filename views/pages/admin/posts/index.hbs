<div class="row">
    <div class="col col-xl-4">
        {{>menu/admin}}
    </div>
    <div class="col col-xl-8" id="field">

            <table class="table">
                <thead>
                <tr>
                    <th scope="col">Название</th>
                    <th scope="col">Категория</th>
                    <th scope="col">Редактор</th>
                    <th scope="col">Дата</th>
                    <th scope="col">Опубликовано</th>
                </tr>
                </thead>
                <tbody>
                {{#result}}
                    <tr>
                        <td><a href="/admin/posts/{{_id}}">{{title}}</a></td>
                        <td>{{category}}</td>
                        <td><a href="{{link}}">{{contact}}</a></td>
                        <td>{{created}}</td>
                        <td>{{published}}</td>
                    </tr>
                {{/result}}
                </tbody>
            </table>
    </div>
</div>